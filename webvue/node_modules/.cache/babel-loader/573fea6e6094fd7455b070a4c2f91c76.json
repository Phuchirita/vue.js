{"ast":null,"code":"import axios from \"axios\";\nconst baseURL = \"http://localhost:3002/todos\";\nexport default {\n  name: \"TodoList\",\n\n  data() {\n    return {\n      todos: [],\n      todoName: \"\"\n    };\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(baseURL);\n      this.todos = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  methods: {\n    async doneTodo(id) {\n      try {\n        await axios.patch(`${baseURL}/${id}`, {\n          done: true\n        });\n        this.todos = this.todos.map(todo => {\n          if (todo.id === id) {\n            todo.done = true;\n          }\n\n          return todo;\n        });\n      } catch (e) {\n        console.error(e);\n      }\n    },\n\n    async addTodo() {\n      try {\n        const res = await axios.post(baseURL, {\n          name: this.todoName\n        });\n        this.todos = [...this.todos, res.data];\n        this.todoName = \"\";\n      } catch (e) {\n        console.error(e);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmBA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,MAAMC,OAAM,GAAI,6BAAhB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,EADF;AAELC,cAAQ,EAAE;AAFL,KAAP;AAID,GAPY;;AAQb,QAAMC,OAAN,GAAgB;AACd,QAAI;AACF,YAAMC,GAAE,GAAI,MAAMP,KAAK,CAACQ,GAAN,CAAUP,OAAV,CAAlB;AAEA,WAAKG,KAAL,GAAaG,GAAG,CAACJ,IAAjB;AACF,KAJA,CAIE,OAAOM,CAAP,EAAU;AACVC,aAAO,CAACC,KAAR,CAAcF,CAAd;AACF;AACD,GAhBY;;AAiBbG,SAAO,EAAE;AACP,UAAMC,QAAN,CAAeC,EAAf,EAAmB;AACjB,UAAI;AACF,cAAMd,KAAK,CAACe,KAAN,CAAa,GAAEd,OAAQ,IAAGa,EAAG,EAA7B,EAAgC;AACpCE,cAAI,EAAE;AAD8B,SAAhC,CAAN;AAIA,aAAKZ,KAAL,GAAa,KAAKA,KAAL,CAAWa,GAAX,CAAeC,IAAG,IAAK;AAClC,cAAIA,IAAI,CAACJ,EAAL,KAAYA,EAAhB,EAAoB;AAClBI,gBAAI,CAACF,IAAL,GAAY,IAAZ;AACF;;AAEA,iBAAOE,IAAP;AACD,SANY,CAAb;AAOF,OAZA,CAYE,OAAOT,CAAP,EAAU;AACVC,eAAO,CAACC,KAAR,CAAcF,CAAd;AACF;AACD,KAjBM;;AAkBP,UAAMU,OAAN,GAAgB;AACd,UAAI;AACF,cAAMZ,GAAE,GAAI,MAAMP,KAAK,CAACoB,IAAN,CAAWnB,OAAX,EAAoB;AAAEC,cAAI,EAAE,KAAKG;AAAb,S