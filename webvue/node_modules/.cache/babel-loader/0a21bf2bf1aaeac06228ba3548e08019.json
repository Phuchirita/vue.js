{"ast":null,"code":"import axios from \"axios\";\nconst baseURL = \"http://localhost:3001/todos\";\nexport default {\n  name: \"TodoList\",\n\n  data() {\n    return {\n      todos: [],\n      todoName: \"\"\n    };\n  },\n\n  async created() {\n    try {\n      const res = await axios.get(baseURL);\n      this.todos = res.data;\n    } catch (e) {\n      console.error(e);\n    }\n  },\n\n  methods: {\n    async addTodo() {\n      try {\n        const res = await axios.post(baseURL, {\n          name: this.todoName\n        });\n        this.todos = [...this.todos, res.data];\n        this.todoName = \"\";\n      } catch (e) {\n        console.error(e);\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmBA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,MAAMC,OAAM,GAAI,6BAAhB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE,EADF;AAELC,cAAQ,EAAE;AAFL,KAAP;AAID,GAPY;;AAQb,QAAMC,OAAN,GAAgB;AACd,QAAI;AACF,YAAMC,GAAE,GAAI,MAAMP,KAAK,CAACQ,GAAN,CAAUP,OAAV,CAAlB;AAEA,WAAKG,KAAL,GAAaG,GAAG,CAACJ,IAAjB;AACF,KAJA,CAIE,OAAOM,CAAP,EAAU;AACVC,aAAO,CAACC,KAAR,CAAcF,CAAd;AACF;AACD,GAhBY;;AAiBbG,SAAO,EAAE;AACP,UAAMC,OAAN,GAAgB;AACd,UAAI;AACF,cAAMN,GAAE,GAAI,MAAMP,KAAK,CAACc,IAAN,CAAWb,OAAX,EAAoB;AAAEC,cAAI,EAAE,KAAKG;AAAb,SAApB,CAAlB;AAEA,aAAKD,KAAL,GAAa,CAAC,GAAG,KAAKA,KAAT,EAAgBG,GAAG,CAACJ,IAApB,CAAb;AAEA,aAAKE,QAAL,GAAgB,EAAhB;AACF,OANA,CAME,OAAOI,CAAP,EAAU;AACVC,eAAO,CAACC,KAAR,CAAcF,CAAd;AACF;AACF;;AAXO;AAjBI,CAAf","names":["axios","baseURL","name","data","todos","todoName","created","res","get","e","console","error","methods","addTodo","post"],"sourceRoot":"","sources":["D:\\testvue\\web\\webvue\\src\\components\\TodoList.vue"],"sourcesContent":["<template>\r\n  <di>\r\n    <h1>Todos</h1>\r\n    <input\r\n      type=\"text\"\r\n      v-model=\"todoName\"\r\n      @keyup.enter=\"addTodo\"\r\n      aria-label=\"Add a new Todo\"\r\n      placeholder=\"Add a new Todo\"\r\n    />\r\n    <ul>\r\n      <li v-for=\"todo of todos\" :key=\"todo.id\">\r\n        {{ todo.